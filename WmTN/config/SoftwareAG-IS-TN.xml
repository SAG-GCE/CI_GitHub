<ehcache     
    name="SoftwareAG.IS.TN"     
    monitoring="on">    
    <diskStore         
        path="./cacheStore/SoftwareAG-IS-TN"/>    
    <defaultCache         
        maxEntriesLocalHeap="0"/>    
    <cache         
        name="TNDocAttribute"         
        maxEntriesLocalHeap="0">        
        <searchable>            
            <searchAttribute                 
                name="attributeName"                 
                expression="value.getName()"/>        
        </searchable>    
    </cache>    
    <cache         
        name="TNDocType"         
        maxEntriesLocalHeap="0">        
        <searchable>            
            <searchAttribute                 
                name="typeName"                 
                class="com.wm.app.tn.cache.attributeExtractors.BizDocTypeCacheSearchAttributeExtractor"/>        
        </searchable>    
    </cache>    
    <cache         
        name="TNProfile"         
        eternal="true"         
        maxEntriesLocalHeap="50"         
        copyOnRead="true"/>    
    <cache         
        name="TNQueryResults"         
        maxEntriesLocalHeap="500"         
        timeToIdleSeconds="300"         
        timeToLiveSeconds="1200"         
        maxEntriesLocalDisk="5000">        
        <persistence             
            strategy="localtempswap"/>    
    </cache>    
    <cache         
        name="TNProfileId"         
        eternal="true"         
        maxEntriesLocalHeap="0"/>    
    <cache         
        name="TNTPAId"         
        eternal="true"         
        maxEntriesLocalHeap="0">        
        <persistence             
            strategy="none"/>    
    </cache>    
    <cache         
        name="TNProfileSummary"         
        eternal="true"         
        maxEntriesLocalHeap="0">        
        <searchable>            
            <searchAttribute                 
                name="displayName"                 
                expression="value.getDisplayName()"/>        
        </searchable>    
    </cache>    
    <cache         
        name="TNTPA"         
        eternal="true"         
        maxEntriesLocalHeap="1000"         
        memoryStoreEvictionPolicy="FIFO">        
        <persistence             
            strategy="none"/>    
    </cache>
</ehcache>
